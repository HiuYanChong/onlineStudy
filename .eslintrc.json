{
  "extends": "eslint-config-egg",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "amd": true
  },
  "rules": {
    "array-bracket-spacing": "off",
    "no-unused-vars": "off",
    "no-else-return": "off",
    "strict": "off",
    "no-alert": "off"
  }
}
